#!/bin/sh

xrandr --output VGA-0 --off
redshift -P -O 3600k &
xrdb ~/.Xresources &
/home/oizero/.config/feh/fehbg_d &
picom --no-fading-openclose --backend glx --config $HOME/.config/picom/picom_d.conf &
/usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1 &
dunst -conf $HOME/.config/dunst/dunstrc_d &
nm-applet &
setxkbmap -option "grp:alt_space_toggle" -layout "us,sk"
slbar &
export CWM='dwm'
source ~/.zshrc &
exec dwm
